<?xml version="1.0" encoding="utf-8"?>
    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.mikhaellopez.circularimageview.CircularImageView
        android:id="@+id/childrenImage"
        android:layout_width="127dp"
        android:layout_height="134dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:src="@drawable/tone"
        app:civ_border_color="#3F5DB6"
        app:civ_border_width="4dp"
        app:civ_shadow="true"
        app:civ_shadow_color="#3f51b5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.384"
        tools:ignore="MissingConstraints" />

    <com.mikhaellopez.circularimageview.CircularImageView
        android:id="@+id/teacherImage"
        android:layout_width="129dp"
        android:layout_height="136dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:src="@drawable/studentthree"
        app:civ_border_color="#374DBF"
        app:civ_border_width="4dp"
        app:civ_shadow="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.385"
        tools:ignore="MissingConstraints" />

    <com.mikhaellopez.circularimageview.CircularImageView
        android:id="@+id/parentImage"
        android:layout_width="129dp"
        android:layout_height="134dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:src="@drawable/parenttwo"
        app:civ_border_color="#3f51b5"
        app:civ_border_width="4dp"
        app:civ_shadow="true"
        app:civ_shadow_color="#3f51b5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.953"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.382"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/teacher_text"
        android:layout_width="111dp"
        android:layout_height="40dp"
        android:fontFamily="@font/amaranth"
        android:text="Teacher"
        android:textColor="#0B0A0A"
        android:textSize="30dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="369dp" />

    <TextView
        android:id="@+id/student_text"
        android:layout_width="119dp"
        android:layout_height="39dp"
        android:fontFamily="@font/amaranth"
        android:text="Student"
        android:textColor="#090909"
        android:textSize="30dp"
        tools:layout_editor_absoluteX="157dp"
        tools:layout_editor_absoluteY="369dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="109dp"
        android:layout_height="37dp"
        android:fontFamily="@font/amaranth"
        android:text="Parent"
        android:textColor="#020202"
        android:textSize="30dp"
        tools:layout_editor_absoluteX="304dp"
        tools:layout_editor_absoluteY="369dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="128dp"
        android:layout_height="57dp"
        android:fontFamily="@font/amaranth"
        android:text="I am a"
        android:textColor="#0B0A0A"
        android:textSize="40dp"
        tools:layout_editor_absoluteX="145dp"
        tools:layout_editor_absoluteY="121dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="414dp"
        android:layout_height="52dp"
        android:fontFamily="@font/amaranth"
        android:text="Let's stepup your account "
        android:textColor="#090909"
        android:textSize="35dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="49dp" />


</androidx.constraintlayout.widget.ConstraintLayout>